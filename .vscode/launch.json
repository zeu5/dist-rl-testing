    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run etcd coverage",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "etcd",
                "cov"
            ]
        },
        {
            "name": "Run etcd hierarchy",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "etcd",
                "hierarchy",
                "LeaderInTerm5"
            ]
        },
        {
            "name": "Run rsl coverage",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "rsl",
                "cov",
                "--episodes", "10000"
            ]
        },
        {
            "name": "Plot coverage python file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["results"]
        },
        {
            "name": "Plot hierarchy coverage python file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["results"]
        }
    ]
}